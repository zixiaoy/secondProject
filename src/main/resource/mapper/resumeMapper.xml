<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaoy.dao.ResumeDao">

    <insert id="saveResume" parameterType="Resume">
        INSERT INTO resume(name,age,sex,education,tel,email,politics_status,department_id,position_id,
        expect_salary,experience,address,interest,speciality,intro,visitor_id) VALUES(#{name},#{age},
        #{sex},#{education},#{tel},#{email},#{politicsStatus},#{departmentId},#{positionId},#{expectSalary},
        #{experience},#{address},#{interest},#{speciality},#{intro},#{visitorId});
    </insert>
</mapper>